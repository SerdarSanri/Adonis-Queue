{
  "name": "@cavai/queue",
  "version": "0.0.1",
  "typings": "./build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "build": "npm run compile; npm run copyfiles",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc-2.1",
    "@adonisjs/mrm-preset": "^3.0.0",
    "@adonisjs/require-ts": "^2.0.2",
    "@types/node": "^14.14.25",
    "copyfiles": "^2.4.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-adonis": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "japa": "^3.1.1",
    "mrm": "^2.5.18",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "license": "MIT",
  "main": "build/providers/QueueProvider.js",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "adonisjs": {
    "types": "@cavai/queue",
    "providers": [
      "@cavai/queue"
    ],
    "templates": {
      "start": [
        {
          "src": "jobs.txt",
          "dest": "jobs"
        }
      ],
      "config": [
        {
          "src": "queue.txt",
          "dest": "queue"
        }
      ]
    }
  },
  "dependencies": {
    "bee-queue": "^1.3.1"
  }
}
