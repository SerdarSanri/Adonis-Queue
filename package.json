{
  "name": "@cavai/adonis-queue",
  "version": "3.0.1",
  "author": "cavai",
  "description": "> Basic AdonisJS queue provider",
  "keywords": [
    "Adonis",
    "AdonisJS",
    "Adonis V5",
    "Queue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cavai-research/Adonis-Queue.git"
  },
  "bugs": {
    "url": "https://github.com/cavai-research/Adonis-Queue/issues"
  },
  "homepage": "https://github.com/cavai-research/Adonis-Queue#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./commands": "./build/commands/main.js",
    "./queue_provider": "./build/providers/queue_provider.js",
    "./queue": "./build/services/queue.js",
    "./define_config": "./build/src/define_config.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy:templates",
    "release": "np",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "fullbuild": "npm run build; npm run postbuild; npx adonis-kit index build/commands"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.0.0",
    "@adonisjs/core": "^6.2.0",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/lucid": "^20.0.0",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/assert": "^2.1.0",
    "@japa/expect-type": "^2.0.1",
    "@japa/runner": "^3.1.1",
    "@swc/core": "^1.3.102",
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.10.7",
    "c8": "^9.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.38.0",
    "luxon": "^3.4.4",
    "np": "^9.2.0",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "superjson": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config"
}
